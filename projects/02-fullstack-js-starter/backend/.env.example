# Backend Environment Variables
# Copy this file to .env and fill in secure values locally.

# Port where Express server listens.
PORT=4000

# PostgreSQL connection string in Prisma format.
DATABASE_URL="postgresql://postgres:postgres@localhost:5432/starter_kit?schema=public"

# Secret used to sign JWT tokens. Use a long random string in production.
JWT_SECRET="dev-secret-change-me"

# Allowed origin for CORS. Update to your frontend URL.
CLIENT_URL="http://localhost:5173"
